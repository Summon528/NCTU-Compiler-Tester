int main() {
    int t, i;
    read t;
    for (i = 0; i < t; i = i+1) {
        int a, b, c, d;
        read a;
        read b;
        read c;
		if ( b < a ) {
			d = a;a = b;b = d;
		}
		if ( c < a ) {
			d = a;a = c;c = d;
		}
		if ( c < b ) {
			d = b;b = c;c = d;
        }
        print "Case ";
        print i + 1;
        print ": ";
        print b;
        print "\n";
    }
    return 0;
}
