void cmp(float a, float b) {
	if( a == b ) {
		print a;
		print " is equal to ";
		print b;
		print "\n";
	}
	
	if( a > b ) {
		print a;
		print " is greater than ";
		print b;
		print "\n";
	}
	
	else {
		print a;
		print " is less than or equal to ";
		print b;
		print "\n";
	}
}

void main(){ 
    cmp(1, 1);
    cmp(1.732, 1.414);
}


/*
Input:
1.732
1.414

Output:
1.732 is greater than 1.414
*/
